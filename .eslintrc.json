{
  "env": {
    "node": true,
    "browser": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": "off",
    "no-plusplus": "off",
    "import/extensions": "off",
    "object-curly-newline": "off",
    "object-curly-spacing": "off",
    "linebreak-style": "off",
    "no-restricted-syntax": "off",
    "no-underscore-dangle": "off",
    "import/prefer-default-export": "off"
  }
}
